{
  "name": "serialport-binding-web-serial-api",
  "version": "1.0.0",
  "description": "Web Serial API binding for Node Serialport",
  "main": "./dist/wsa-sp-bind.js",
  "types": "./dist/wsa-sp-bind.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start:dev": "webpack --mode=development --watch --progress",
    "build:prod": "webpack --mode=production",
    "prepublishOnly": "webpack --mode=production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathanjel/serialport-binding-web-serial-api.git"
  },
  "keywords": [
    "serialport",
    "webserialapi",
    "binding"
  ],
  "author": "Marcin Galczynski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nathanjel/serialport-binding-web-serial-api/issues"
  },
  "homepage": "https://github.com/nathanjel/serialport-binding-web-serial-api#readme",
  "dependencies": {
    "@serialport/binding-abstract": "^9.0.2",
    "@serialport/stream": "^9.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@types/serialport": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0"
  }
}
